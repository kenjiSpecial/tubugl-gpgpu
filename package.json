{
  "name": "tubugl-gpgpu",
  "version": "1.3.3",
  "description": "gpgpu util for tubugl",
  "main": "build/tubu-gpgpu.js",
  "cjs": "build/tubu-gpgpu.cjs.js",
  "module": "build/tubu-gpgpu.esm.js",
  "libName": "Tubu",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "00": "parcel examples/app00/index.html --no-hmr",
    "01": "parcel examples/app01/index.html --no-hmr",
    "02": "parcel examples/app02/index.html --no-hmr",
    "03": "parcel examples/app03/index.html --no-hmr",
    "04": "parcel examples/app04/index.html --no-hmr",
    "dist00": "parcel build examples/app00/index.html -d docs/app00 --public-url ./",
    "dist01": "parcel build examples/app01/index.html -d docs/app01 --public-url ./",
    "dist02": "parcel build examples/app02/index.html -d docs/app02 --public-url ./",
    "dist03": "parcel build examples/app03/index.html -d docs/app03 --public-url ./",
    "dist04": "parcel build examples/app04/index.html -d docs/app03 --public-url ./",
    "screenshot00": "node ./node/screenshot.js --dir=app00",
    "screenshot01": "node ./node/screenshot.js --dir=app01",
    "screenshot02": "node ./node/screenshot.js --dir=app02",
    "docs": "python -m SimpleHTTPServer 8000"
  },
  "dependencies": {
    "tubugl-3d-shape": "^1.6.1",
    "tubugl-camera": "^1.3.3",
    "tubugl-constants": "^1.1.1",
    "tubugl-core": "^1.4.5",
    "tubugl-gpgpu": "^1.1.0",
    "tubugl-helper": "^1.1.0",
    "tubugl-math": "^1.1.10",
    "tubugl-utils": "^1.0.9"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babelrc-rollup": "^3.0.0",
    "gsap": "^1.20.4",
    "minimist": "^1.2.0",
    "parcel-plugin-glsl": "^0.1.0",
    "puppeteer": "^0.13.0",
    "rollup": "^0.57.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "uil": "^1.0.1"
  }
}
